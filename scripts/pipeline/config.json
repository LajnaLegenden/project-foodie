{
    "main": {
        "daemon": "unix://var/run/docker.sock",
        "hostname": "project-foodie-prod",
        "environment": "prod",
        "port": 8103,
        "data": "/mnt/user/appdata/data/project-foodie/data",
        "url": "https://prodfoodie.linusjansson.com/",
        "env" : ".env.main"
    },
    "dev": {
        "daemon": "unix://var/run/docker.sock",
        "hostname": "project-foodie-dev",
        "environment": "dev",
        "port": 8101,
        "data": "/mnt/user/appdata/data/project-foodie-dev/data",
        "url": "https://devfoodie.linusjansson.com/",
        "env" : ".env.dev"
    },
    "default": {
        "daemon": "unix://var/run/docker.sock",
        "hostname": "project-foodie-test-branch",
        "environment": "dev",
        "port": 8101,
        "data": "/dev/shm/data",
        "env": ".env"
    }
}
